24) import math
r1 = float(input(&quot;Радиус первой окружности: &quot;))
r2 = float(input(&quot;Радиус второй окружности: &quot;))
D = (2 * r1) + (2 * r2)
S = 1/4 * 3.14 * (D ** 2)
print(&quot;Площадь круга: &quot;, S)
29) import math
C = float(input(&quot;Введите C: &quot;))
R = C / (2 * 3.14)
S = 2 * 3.14 * (R ** 2)
print(&quot;Площадь круга: &quot;, S)

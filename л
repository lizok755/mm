import math
n = int(input(&quot;Введите n: &quot;))
b2 = int(input(&quot;Введите b2: &quot;))
b5 = int(input(&quot;Введите b5: &quot;))
q = (b5 / b2) ** (1/3)
b1 = b2 / q
S = (b1 * (1 - q)) / (1 - q)

print(&quot;Сумма геометрической прогрессии: &quot;, S)
